<%= content_for :title, t('.title') %>
<%= header t '.title' %>
<table class='table table-striped'>
  <thead>
  <tr>
    <th><%= t '.username' %></th>
    <th></th>
  </tr>
  </thead>
  <tfoot></tfoot>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= edit_link_to user.username, edit_admin_user_path(user) %></td>
        <td>
          <%= delete_button_for admin_user_path(user), user.username %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>